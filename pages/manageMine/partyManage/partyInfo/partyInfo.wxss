page{
  width: 100%;
  height: 100%;
}
.partyInfo{
  height: 100%;
  margin-left: 15px;
  margin-right: 15px;
}
.header{
  height: 16%;
  margin-top: 5px;
  border: 1px solid #BEEEEE;
  border-radius: 7px;
  display: flex;
  flex-direction: row;
}
.header-image{
  width: 35%;
  height: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
}
.header-image image{
  width: 40px;
  height: 40px;
}
.header-detail{
  width: 65%;
  display: flex;
  flex-direction: column;
  justify-content: center;
}
.header-detail-name{
  height: 25%;
  font-size: 17px;
  font-weight: bold;
}
.header-detail-class{
  height: 20%;
}
.header-detail-stuNumber{
  height: 20%;
}

.table{
  height: 75%;
  margin-top: 10px;
}
.table-title{
  height: 23px;
  background: #E71111;
  color: #ffffff;
  display: flex;
  flex-direction: row;
}
.table-title-number{
  width: 13%;
  border: 1px solid #BBBBBE;
  display: flex;
  justify-content: center;
  align-items: center;
}
.table-title-name{
  width: 20%;
  display: flex;
  justify-content: center;
  align-items: center;
  border: 1px solid #BBBBBE;
}
.table-title-class{
  width: 33%;
  display: flex;
  justify-content: center;
  align-items: center;
  border: 1px solid #BBBBBE;
}
.table-title-tele{
  width: 33%;
  display: flex;
  justify-content: center;
  align-items: center;
  border: 1px solid #BBBBBE;
}
.table-scrollView{
  height: 95%;
}
.table-item{
  height: 23px;
  display: flex;
  flex-direction: row;
}
.table-item-number{
  width: 13%;
  border: 1px solid #BBBBBE;
  display: flex;
  justify-content: center;
  align-items: center;
}
.table-item-name{
  width: 20%;
  border: 1px solid #BBBBBE;
  display: flex;
  justify-content: center;
  align-items: center;
}
.table-item-class{
  width: 33%;
  border: 1px solid #BBBBBE;
  display: flex;
  justify-content: center;
  align-items: center;
}
.table-item-tele{
  width: 33%;
  border: 1px solid #BBBBBE;
  display: flex;
  justify-content: center;
  align-items: center;
}