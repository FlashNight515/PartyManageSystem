<view class="partyMember">
  <view class="activity-choose">
    <picker class="activity-choose-academy" bindchange="changeDangWei" value="{{dangweiIndex}}" range="{{dangweiArray}}">
      <view class="activity-choose-academy-detail">
        {{dangweiArray[dangweiIndex]}}<image src="/images/下拉.png"></image>
      </view>
    </picker>
    <picker class="activity-choose-academy" bindchange="changeDangZong" value="{{dangzongIndex}}" range="{{dangzongArray}}">
      <view class="activity-choose-academy-detail">
        {{dangzongArray[dangzongIndex]}}<image src="/images/下拉.png"></image>
      </view>
    </picker>
    <picker class="activity-choose-academy" bindchange="changeBranch" value="{{branchIndex}}" range="{{branchArray}}">
      <view class="activity-choose-academy-detail">
        {{branchArray[branchIndex]}}<image src="/images/下拉.png"></image>
      </view>
    </picker>
    <view class="activity-add" bindtap="tapAdd">
      <image src="/images/新建.png"></image>
      <view class="activity-add-contain">添加成员</view>
    </view>
  </view>
  <view class="search">
    <view class="search-name"><input placeholder="姓名"></input></view>
    <view class="search-tele"><input placeholder="手机号"></input></view>
    <view class="search-button" bindtap="tapSearch"><image src="/images/搜索.png" ></image></view>
  </view>
  <view class="table">
    <view class="table-title">
        <view class="table-title-number">序号</view>
        <view class="table-title-name">姓名</view>
        <view class="table-title-position">职位</view>
        <view class="table-title-zhicheng">职称</view>
        <view class="table-title-tele">电话</view>
      </view>
      <scroll-view class="table-scrollView" scroll-y="true">
        <view class="table-item" wx:for='{{arrayMasses}}'>
          <view class="table-item-number">1</view>
          <view class="table-item-name">王晨宝</view>
          <view class="table-item-position">管理员</view>
          <view class="table-item-zhicheng">副教授</view>
          <view class="table-item-tele">15079617331</view>
        </view>
      </scroll-view>
  </view>
</view>