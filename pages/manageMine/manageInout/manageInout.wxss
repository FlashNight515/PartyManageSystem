page{
  width: 100%;
  height: 100%;
}
.inout{
  height: 100%;
  margin-left: 15px;
  margin-right: 15px;
  display: flex;
  flex-direction: column;
}
.untreated{
  height: 45%;
  display: flex;
  flex-direction: column;
}
.untreated-title{
  height: 7%;
  width: 60px;
  border-radius: 5px;
  color: #ffffff;
  background-color: #837D7D;
  display: flex;
  justify-content: center;
  align-items: center;
}
.untreated-contain{
  height: 90%;
  width: 100%;
  display: flex;
  flex-direction: column;
}
.untreated-contain-detail{
  height: 35px;
  margin-top: 10px;
  padding-right: 15px;
  border:1px solid #837D7D;
  border-radius: 5px;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
}
.untreated-contain-detail image{
  width: 15px;
  height: 15px;
}
.treated{
  height:50%;
  display: flex;
  flex-direction: column;
}
.treated-title{
  height: 7%;
  color: #ffffff;
  width: 60px;
  border-radius: 5px;
  background-color: #837D7D;
  display: flex;
  justify-content: center;
  align-items: center;
}
.treated-contain{
  height: 90%;
  padding-top: 5px;
}
.table{
  width: 100%;
  height: 100%;
}
.table-title{
  height: 30px;
  background: #E71111;
  color: #ffffff;
  display: flex;
  flex-direction: row;
}
.table-title-number{
  width: 10%;
  border: 1px solid #DBDBDB;
  display: flex;
  justify-content: center;
  align-items: center;
}
.table-title-name{
  width:16%;
  display: flex;
  justify-content: center;
  align-items: center;
  border: 1px solid #DBDBDB;
}
.table-title-belong{
  width: 37%;
  display: flex;
  justify-content: center;
  align-items: center;
  border: 1px solid #DBDBDB;
}
.table-title-goto{
  width: 37%;
  display: flex;
  justify-content: center;
  align-items: center;
  border: 1px solid #DBDBDB;
}
.table-scrollView{
  height: 95%;
}
.table-item{
  height: 23px;
  display: flex;
  flex-direction: row;
}
.table-item-number{
  width: 10%;
  border: 1px solid #DBDBDB;
  display: flex;
  justify-content: center;
  align-items: center;
}
.table-item-name{
  width: 16%;
  border: 1px solid #DBDBDB;
  display: flex;
  justify-content: center;
  align-items: center;
}
.table-item-belong{
  width: 37%;
  border: 1px solid #DBDBDB;
  display: flex;
  justify-content: center;
  align-items: center;
}
.table-item-goto{
  width: 37%;
  border: 1px solid #DBDBDB;
  display: flex;
  justify-content: center;
  align-items: center;
}