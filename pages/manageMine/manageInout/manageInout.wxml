<view class="inout">
  <view class="untreated">
    <view class="untreated-title">待处理：</view>
    <scroll-view class="untreated-contain" scroll-y='true'>
      <view wx:for='{{arrayInout}}' wx:key='key'>
        <view class="untreated-contain-detail" bindtap="tapUntreatedInout" id='{{index}}'>
          {{item.inOut_userName}}申请<view wx:if="{{item.inOut_isPass==1}}">转入</view><view wx:else>转出</view>
          <image src="/images/右箭头.png" ></image>
        </view>
      </view>
    </scroll-view>
  </view>
  <view class="line"></view>
  <view class="treated">
    <view class="treated-title">已处理：</view>
    <view class="treated-contain">
      <view class="table">
        <view class="table-title">
          <view class="table-title-number">序号</view>
          <view class="table-title-name">姓名</view>
          <view class="table-title-belong">所属支部</view>
          <view class="table-title-goto">去向支部</view>
        </view>
        <scroll-view class="table-scrollView" scroll-y="true">
          <view class="table-item" wx:for='{{arrayPayed}}' bindtap="tapPayed" wx:key='key'>
            <view class="table-item-number">{{index+1}}</view>
            <view class="table-item-name"></view>
            <view class="table-item-belong"></view>
            <view class="table-item-goto"></view>
          </view>
        </scroll-view>
      </view>
    </view>
  </view>
</view>