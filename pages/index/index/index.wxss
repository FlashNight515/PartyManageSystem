.swiper-image{
  width: 100%;
  height: 160rpx;
}
.container{
  width: 100%;
  height: 160px;
  display: flex;
  flex-direction: column;
}
.container-item{
  width: 100%;
  height: 80px;
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  align-items: center;
}
.container-item-applyParty{
  width: 45px;
  height: 45px;
  background-color: #E71111;
  border-radius: 5px;
  display: flex;
  flex-direction: column;
  justify-content: center;
}
.container-item-applyParty-image{
  text-align: center;
}
.container-item-applyParty-image image{
  width: 20px;
  height: 20px;
  }
.container-item-applyParty label{
  width: 100%;
  color: #ffffff;
  font-size: 8px;   
  font-weight:bold;  
  text-align: center;
  margin-top: 3px;
  }
