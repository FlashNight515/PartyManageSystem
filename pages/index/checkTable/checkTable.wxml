<!--pages/index/checkTable/checkTable.wxml-->
<view class="checkTable">
  <form class="form" bindsubmit="formSubmit">
    <view class="name"><view class="name-label">姓名：</view><input name="name" class="name-input"></input></view>
    <view class="name"><view class="name-label3">何时何地毕业：</view><input name="graduate" class="name-input3"></input></view>
    <view class="name"><view class="name-label">学历：</view><input name="education" class="name-input"></input></view>
    <view class="name"><view class="name-label3">参加工作时间：</view><input name="jobTime" class="name-input3"></input></view>
    <view class="name"><view class="name-label3">单位职务职业：</view><input name="job" class="name-input3"></input></view>
    <view class="name"><view class="name-label2">有何特长：</view><input name="speciality" class="name-input2"></input></view>
    <view class="name2"><view class="name-label4">家庭成员情况：</view><textarea name="family" class="name-input4"></textarea></view>
    <view class="name2"><view class="name-label4">主要社会关系情况：</view><textarea name="society" class="name-input4"></textarea></view>
    <view class="name2"><view class="name-label4">历次申请情况：</view><textarea name="applyHistory" class="name-input4"></textarea></view>
    <view class="name2"><view class="name-label4">何时何地参加过何种组织任何职：</view><textarea name="work" class="name-input4"></textarea></view>
    <view class="name2"><view class="name-label4">何时何地受过何种奖励：</view><textarea name="award" class="name-input4"></textarea></view>
    <view class="name2"><view class="name-label4">何时何地何原因受过何种处分：</view><textarea name="punishment" class="name-input4"></textarea></view>
    <view class="btn-submit"><button form-type="submit">确认提交</button> </view>
  </form>
</view>
