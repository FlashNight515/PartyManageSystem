/* pages/index/checkTable/checkTable.wxss */
page{
  width: 100%;
}
.checkTable{
  margin-left: 30px;
  margin-right: 30px;
}
.form{
  width: 100%;
  height: 100%;
}
.name{
  height: 30px;
  width: 100%;
  margin-top: 7px;
  margin-bottom: 7px;
  display: flex;
  flex-direction: row;
}
.name-label{
  height: 100%;
  width: 15%;
  display: flex;
  align-items: center;
}
.name-input{
  width: 85%;
  height: 100%;
  border: 1px solid #Beeeee;
  border-radius: 5px;
}
.name-label2{
  height: 100%;
  width: 30%;
  display: flex;
  align-items: center;
}
.name-input2{
  height: 100%;
  width: 70%;
  border: 1px solid #Beeeee;
  border-radius: 5px;
}
.name-label3{
  height: 100%;
  width: 32%;
  display: flex;
  align-items: center;
}
.name-input3{
  height: 100%;
  width: 68%;
  border: 1px solid #Beeeee;
  border-radius: 5px;
}
.name2{
  height: 100px;
  width: 100%;
  margin-top: 7px;
  margin-bottom: 7px;
  display: flex;
  flex-direction: column;
}
.name-label4{
  height: 20%;
  width: 100%;
  display: flex;
  align-items: center;
}
.name-input4{
  height: 80%;
  width: 100%;
  border: 1px solid #Beeeee;
  border-radius: 5px;
}
.btn-submit{
  margin-top: 15px;
  height: 70px;
}
.btn-submit button{
  color: #ffffff;
  background-color: #e71111;
}
