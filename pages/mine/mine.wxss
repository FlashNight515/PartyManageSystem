.mine-head{
  height: 150px;
  margin-left: 15px;
  margin-right: 15px;
  border: 1px solid #743833;
  background-color: #BBBBBE;
  border-radius: 10px;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.mine-head-image{
  padding-top: 5px;
  height: 47%;
}
.mine-head-image image{
  width: 50px;
  height: 50px;
}
.mine-head-name{
  height: 15%;
  font-size: 13px;
}
.mine-head-branch{
  height: 15%;
  font-size: 13px;
}
.mine-head-stage{
  height: 15%;
  font-size: 13px;
}
.mine-contain{
  margin-left: 15px;
  margin-right: 15px; 
  margin-top: 40px;
}
.mine-contain-item{
  height: 40px;
  display: flex;
  flex-direction: row;
}
.mine-contain-item-image{
  width: 15%;
}
.mine-contain-item-image image{
  width: 15px;
  height: 15px;
  padding-left: 15px;
}
.mine-contain-item-title{
  width: 75%;
  font-size: 14px;
}
.mine-contain-item-enter{
  width: 10%;
}
.mine-contain-item-enter image{
  width: 15px;
  height: 15px;
}